import java.util.*;
class decimalg
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int a[]=new int[20];
System.out.println("enter the no to digits in binary no");
int n=sc.nextInt();
for(int i=0;i<n;i++)
{
a[i]=sc.nextInt();
}
int i=0;
while(i<n)
{
for(int k=n-1;k>=0;k--)
{
double result=0;
 result=result + a[i]*math.pow(2,k);
i++;
}
}
System.out.println("the decimal no is" +result);
}
}
